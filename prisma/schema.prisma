// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id Int @id @default(autoincrement())
  username String 
  email String
  role String
  dateCreate DateTime @default(now())
}

model Profile {
  id Int @id @default(autoincrement())
  firstName String
  lastName String
  state String
}